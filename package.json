{
  "name": "react-vite-styled-components",
  "description": "A very simple repo using React, Vite and Styled Components",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "vite build",
    "release": "standard-version"
  },
  "keywords": [
    "react",
    "boilerplate",
    "typescript",
    "starter"
  ],
  "author": "Paddy Griffin",
  "repository": {
    "type": "git",
    "url": "https://github.com/paddykgriffin/react-vite-styled-components-starter.git "
  },
  "dependencies": {
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@reduxjs/toolkit": "^2.2.3",
    "normalize.css": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.4.3",
    "redux-persist": "^6.0.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sass": "1.54.9",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.23.0",
    "vite": "3.2.4",
    "vite-plugin-checker": "0.5.1",
    "vite-plugin-html": "3.2.0",
    "vite-tsconfig-paths": "3.5.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": "last 2 versions"
}
